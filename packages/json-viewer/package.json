{
  "name": "@availity/json-viewer",
  "version": "1.2.5",
  "description": "React component that renders JSON data in an accessible way",
  "keywords": [
    "react",
    "availity",
    "json"
  ],
  "homepage": "https://availity.github.io/availity-react/components/json-viewer",
  "bugs": {
    "url": "https://github.com/Availity/availity-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Availity/availity-react.git",
    "directory": "packages/json-viewer"
  },
  "license": "MIT",
  "author": "Ben Newman <benjamin.newman@availity.com>",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf dist",
    "clean:nm": "rm -rf node_modules",
    "publish": "yarn npm publish --tolerate-republish --access public",
    "publish:canary": "yarn npm publish --access public --tag canary"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
