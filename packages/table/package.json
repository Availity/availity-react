{
  "name": "@availity/table",
  "version": "0.6.17",
  "description": "A generic table wrapper for react-table",
  "keywords": [
    "react",
    "availity"
  ],
  "bugs": {
    "url": "https://github.com/Availity/availity-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Availity/availity-react.git"
  },
  "license": "MIT",
  "author": "Ashlee Zeigler <ashlee.zeigler@availity.com>",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf dist",
    "clean:nm": "rm -rf node_modules",
    "publish": "yarn npm publish --tolerate-republish --access public",
    "publish:canary": "yarn npm publish --access public --tag canary"
  },
  "dependencies": {
    "@availity/hooks": "workspace:*",
    "@availity/icon": "workspace:*",
    "@types/react-table": "^7.7.20",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react-table": "^7.8.0"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactstrap": "^8.10.1",
    "tsup": "^8.4.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "availity-uikit": ">=4.7.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "reactstrap": "^8.10.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
