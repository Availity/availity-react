{
  "name": "@availity/block-ui",
  "version": "1.1.0",
  "description": "Block UI for react",
  "keywords": [
    "availity",
    "loader",
    "blockui",
    "react"
  ],
  "homepage": "https://availity.github.io/availity-react/components/block-ui",
  "bugs": {
    "url": "https://github.com/Availity/availity-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Availity/availity-react.git",
    "directory": "packages/block-ui"
  },
  "license": "MIT",
  "author": "Jordan Young <jordan.young@availity.com>",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf dist",
    "clean:nm": "rm -rf node_modules",
    "bundlesize": "bundlesize",
    "publish": "yarn npm publish --tolerate-republish --access public",
    "publish:canary": "yarn npm publish --access public --tag canary"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
